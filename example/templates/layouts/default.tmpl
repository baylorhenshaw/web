{{ define "default" }}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>{{ .Head.Title }}</title>

    {{ if .Head.Analytics.Enabled }}
        <script defer src="https://{{ .Head.Analytics.Url }}/script.js" data-website-id="{{ .Head.Analytics.Id }}"></script>
    {{ end }}

    <h4>{{ .Head.Analytics.Enabled }}</h4>
</head>
<body>
    {{ template "page" . }}
</body>
</html>

{{ end }}